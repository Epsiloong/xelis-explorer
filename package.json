{
  "name": "xelis-explorer",
  "version": "0.1.0",
  "scripts": {
    "start": "concurrently \"npm run build:local_dev\" \"npm run serve\"",
    "serve": "live-server ./public --port=9999 --entry-file=index.html --proxy=/node-proxy:http://127.0.0.1:3010",
    "build:local_dev": "node bundle.js --env=local_dev --watch --sourcemap",
    "build:dev": "node bundle.js --env=dev --sourcemap",
    "build:testnet": "node bundle.js --env=testnet --sourcemap",
    "build:mainnet": "node bundle.js --env=mainnet --minify"
  },
  "dependencies": {
    "animate.css": "^4.1.1",
    "await-to-js": "^3.0.0",
    "bytes": "^3.1.2",
    "chart.js": "^4.2.1",
    "classnames": "^2.3.2",
    "concurrently": "^7.6.0",
    "css.gg": "^2.0.0",
    "pretty-ms": "^8.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-router": "^6.7.0",
    "react-router-dom": "^6.7.0",
    "reset-css": "^5.0.1",
    "store2": "^2.14.2"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.13",
    "esbuild": "^0.17.10",
    "esbuild-style-plugin": "^1.6.1",
    "postcss": "^8.4.21",
    "postcss-import": "^15.1.0",
    "postcss-prefix-selector": "^1.16.0",
    "serve": "^14.2.0",
    "yargs": "^17.7.0"
  }
}
