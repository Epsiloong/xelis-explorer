{
  "name": "xelis-explorer",
  "version": "0.1.0",
  "scripts": {
    "start": "concurrently \"npm run build -- --watch --sourcemap=inline\" \"npm run serve\"",
    "serve": "live-server ./public --port=3000 --entry-file=index.html",
    "build": "esbuild ./src/index --loader:.js=jsx --loader:.woff=text --loader:.woff2=text --jsx=automatic --bundle --outdir=public/dist",
    "build-prod": "npm run build -- --minify"
  },
  "dependencies": {
    "classnames": "^2.3.2",
    "concurrently": "^7.6.0",
    "css.gg": "^2.0.0",
    "esbuild": "^0.17.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router": "^6.7.0",
    "react-router-dom": "^6.7.0",
    "reset-css": "^5.0.1",
    "serve": "^14.1.2",
    "store2": "^2.14.2"
  }
}
