{
  "name": "xelis-explorer",
  "version": "0.2.0",
  "description": "Official Explorer for Xelis Blockchain.",
  "scripts": {
    "start": "concurrently \"npm run build:local_dev\" \"npm run serve\"",
    "start:testnet": "concurrently \"npm run build:local_testnet\" \"npm run serve\"",
    "serve": "live-server ./public --port=9999 --entry-file=index.html",
    "build:local_dev": "node bundle.js --env=local_dev --watch --sourcemap",
    "build:dev": "node bundle.js --env=dev --sourcemap",
    "build:local_testnet": "node bundle.js --env=testnet --watch --sourcemap",
    "build:testnet": "node bundle.js --env=testnet --minify",
    "build:mainnet": "node bundle.js --env=mainnet --minify"
  },
  "dependencies": {
    "@react-three/drei": "^9.57.0",
    "@react-three/fiber": "^8.11.9",
    "@supabase/supabase-js": "^2.10.0",
    "@xelis/sdk": "^0.2.0",
    "await-to-js": "^3.0.0",
    "bytes": "^3.1.2",
    "chart.js": "^4.2.1",
    "classnames": "^2.3.2",
    "concurrently": "^7.6.0",
    "css.gg": "^2.1.1",
    "framer-motion-3d": "^10.1.0",
    "goober": "^2.1.10",
    "hash-it": "^6.0.0",
    "pretty-ms": "^8.0.0",
    "query-string": "^8.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^1.3.0",
    "react-router": "^6.7.0",
    "react-router-dom": "^6.7.0",
    "reset-css": "^5.0.1",
    "store2": "^2.14.2",
    "three": "^0.150.1"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.13",
    "esbuild": "^0.17.10",
    "esbuild-style-plugin": "^1.6.1",
    "postcss": "^8.4.21",
    "postcss-import": "^15.1.0",
    "postcss-prefix-selector": "^1.16.0",
    "serve": "^14.2.0",
    "yargs": "^17.7.0"
  }
}
